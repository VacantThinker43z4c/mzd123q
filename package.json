{
  "name": "desktopnaotu",
  "version": "0.1.2",
  "description": "桌面版脑图",
  "main": "main.js",
  "scripts": {
    "start": "gulp default && electron main.js",
    "packwin32": "electron-packager ./ DesktopNaotu --platform=win32 --arch=ia32 --out=../OutApp --icon=favicon.ico --version=1.6.4 --ignore=\"(.git|node_modules)\" --overwrite --prune",
    "packwin64": "electron-packager ./ DesktopNaotu --platform=win32 --arch=x64 --out=../OutApp --icon=favicon.ico --version=1.6.4 --ignore=\"(.git|node_modules)\" --overwrite --prune",
    "packosx": "electron-packager ./ DesktopNaotu --platform=darwin --arch=x64 --out=../OutApp --icon=favicon.icns --version=1.6.4 --ignore=\"(.git|node_modules)\" --overwrite --prune",
    "packlinux": "electron-packager ./ DesktopNaotu --platform=linux --arch=x64 --out=../OutApp --icon=favicon.icns --version=1.6.4 --ignore=\"(.git|node_modules)\" --overwrite --prune"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/topcss/DesktopNaotu.git"
  },
  "keywords": ["mind", "naotu"],
  "author": "'Jack' <'topcss@gmail.com'>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/topcss/DesktopNaotu/issues"
  },
  "homepage": "https://github.com/topcss/DesktopNaotu#readme",
  "devDependencies": {
    "del": "^2.2.2",
    "electron": "^1.4.5",
    "electron-prebuilt": "^1.4.5",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-cache": "^0.4.5",
    "gulp-clean-css": "^2.0.13",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^5.0.0",
    "gulp-imagemin": "^3.1.1",
    "gulp-jshint": "^2.0.2",
    "gulp-livereload": "^3.8.1",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-ruby-sass": "^2.1.1",
    "gulp-uglify": "^2.0.0",
    "js-beautify": "^1.6.4",
    "run-sequence": "^1.2.2",
    "wiredep": "^4.0.0"
  }
}